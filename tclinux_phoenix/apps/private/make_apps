	$(MAKE) -C $(APP_PRIVATE_DIR)/API/cmd/ecnt_igmp_cmd
	if test -e $(APP_PRIVATE_DIR)/API/cmd/ecnt_igmp_cmd ; \
	then echo "Compile ecnt_igmp_cmd cmd success"; \
	else echo "Compile ecnt_igmp_cmd cmd error!"; exit 1; \
	fi
endif

