	echo "Clean libupnp"
	-[ -f $(APP_LIBUPNP_DIR)/Makefile ] && make AUTOCONF=: AUTOMAKE=: -C  $(APP_LIBUPNP_DIR) clean distclean
	- rm -rf $(APP_LIBUPNP_DIR)/install

