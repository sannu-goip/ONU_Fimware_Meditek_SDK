	$(STRIP) $(SFTP_DIR)/ssh
	$(STRIP) $(SFTP_DIR)/sshd
	$(STRIP) $(SFTP_DIR)/ssh-add
	$(STRIP) $(SFTP_DIR)/ssh-agent
	$(STRIP) $(SFTP_DIR)/ssh-keygen
	$(STRIP) $(SFTP_DIR)/ssh-keyscan
