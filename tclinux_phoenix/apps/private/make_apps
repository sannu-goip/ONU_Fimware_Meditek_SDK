apps_cp_before_cleanfs:
	echo "don't do apps_cp_before_cleanfs"
xpon_app_clean:
xpon_app:
else
include $(XPON_APP_SDK_DIR)/make_xpon_app
apps_cp_before_cleanfs:
	chmod -R 777 $(APP_DIR)
endif
