ifneq ($(strip $(TCSUPPORT_DRIVER_API)),)
	echo "Build API cmd."
ifneq ($(strip $(TCSUPPORT_QDMA_BUFMGR)),)
ifeq ($(strip $(TCSUPPORT_NP)),)
